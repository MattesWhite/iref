## IriRef

[x] iref = iref
[x] iref = iref_buf
[x] iref = iri
[x] iref = iri_buf

[ ] iref <> iref
[ ] iref <> iref_buf
[ ] iref <> iri
[ ] iref <> iref_buf

[ ] iref clone, copy
[ ] iref from &iref_buf
[ ] iref from iri
[ ] iref from &iri_buf

[ ] iref hash

## IriRefBuf

iref_buf = iref
iref_buf = iref_buf
iref_buf = iri
iref_buf = iri_buf

iref_buf <> iref
iref_buf <> iref_buf
iref_buf <> iri
iref_buf <> iref_buf

iref_buf clone
iref_buf from iref
iref_buf from iri
iref_buf from iri_buf
iref_buf from &iri_buf

iref_buf hash

## Iri

iri = iref
iri = iref_buf
iri = iri
iri = iri_buf

iri <> iref
iri <> iref_buf
iri <> iri
iri <> iref_buf

iri clone, copy
iri from &iri_buf
iri try from &iref_buf
iri try from iref

iri hash

## IriBuf

iri_buf = iref
iri_buf = iref_buf
iri_buf = iri
iri_buf = iri_buf

iri_buf <> iref
iri_buf <> iref_buf
iri_buf <> iri
iri_buf <> iref_buf

iri_buf clone
iri_buf from iri
iri_buf try from iref
iri_buf try from iref_buf
iri_buf try from &iref_buf

iri_buf hash
